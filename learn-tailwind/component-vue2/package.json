{
  "name": "@xieyuheng/tailwind-components-vue2",
  "version": "0.1.14",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --mode production src/components/index.ts --target lib --dest lib --name components",
    "build-wc": "vue-cli-service build --mode production 'src/components/*.vue' --target wc --dest wc --name twc --inline-vue",
    "format": "prettier src --write"
  },
  "main": "lib/components.umd.js",
  "files": [
    "src",
    "lib",
    "wc"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "twind": "^0.16.16",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.1.2",
    "windicss": "^3.1.7"
  },
  "devDependencies": {
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "prettier": "^2.3.2",
    "prettier-plugin-tailwind-css": "^1.5.0",
    "typescript": "~4.1.5",
    "vue-cli-plugin-windicss": "~1.1.6",
    "vue-template-compiler": "^2.6.11"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "types": "src/components/index.ts"
}
